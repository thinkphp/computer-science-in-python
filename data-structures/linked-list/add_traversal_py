class Node:
    
    def __init__(self, data):
        self.data = data
        self.next = None

class LinkedList:

      def __init__(self):

          self.head = None
      def insert2(self, value):

          newNode = Node(value)

          if self.head is None:
              self.head = newNode
          else:
              curr = self.head
              while curr.next is not None:
                  curr = curr.next
              curr.next = newNode

      def insert(self, value):

          newNode = Node(value)

          if self.head is None:
              self.head = newNode
          else:
              newNode.next = self.head
              self.head = newNode

      def traversal(self):
          c = self.head
          while c is not None:
              print(c.data, end = " ")
              c = c.next

def main():
    obj = LinkedList()
    obj.insert2(75)
    obj.insert2(22)
    obj.insert2(13)
    obj.traversal()
main()
